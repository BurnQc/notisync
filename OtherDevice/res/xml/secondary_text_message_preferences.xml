<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:holo="http://schemas.android.com/apk/res-auto"
    holo:title="Text Messages" >

    <CheckBoxPreference
        holo:key="secondary_text_message_enabled"
        holo:title="Enabled" />
    <CheckBoxPreference
        holo:key="secondary_text_message_group_notifications"
        holo:title="Group Notifications" />

    <RingtonePreference
        holo:ringtoneType="notification"
        holo:showDefault="true"
        holo:showSilent="true"
        holo:key="secondary_text_message_ringtone"
        holo:title="Notification ringtone" />

    <CheckBoxPreference
        holo:key="secondary_text_message_vibrate"
        holo:title="Vibrate" />
    <CheckBoxPreference
        holo:key="secondary_text_message_lights"
        holo:title="Lights" />

</PreferenceScreen>